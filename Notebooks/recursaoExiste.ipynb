   "source": [
    "A = [4, 2, 1, 5, 6, 9]\n",
    "\n",
    "def existe(n, v):\n",
    "    if n == 0:\n",
    "        return True if A[n] == v else False\n",
    "    else:\n",
    "        f = existe(n - 1, v)\n",
    "        return f or v == A[n - 1]\n",
    "\n",
    "existe(len(A), 1)"
   ]
