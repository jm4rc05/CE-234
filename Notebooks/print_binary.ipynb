   "source": [
    "def int_to_binary(i):\n",
    "    s = []\n",
    "    while(i > 0):\n",
    "        d = i % 2\n",
    "        s.append(d)\n",
    "        i = i // 2\n",
    "\n",
    "    print(s)\n",
    "    return s\n",
    "\n",
    "def print_binary(s):\n",
    "    while(len(s) > 0):\n",
    "        d = s.pop()\n",
    "        print(d)\n",
    "    \n",
    "x = int_to_binary(2)\n",
    "\n",
    "print_binary(x)\n"
   ]
