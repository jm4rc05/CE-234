   "source": [
    "A = [8, 12, 26, 24, 15]\n",
    "\n",
    "def maximo(n):\n",
    "    if n == 0:\n",
    "        return A[0]\n",
    "    m = maximo(n - 1)\n",
    "    return m if m > A[n] else A[n]\n",
    "\n",
    "maximo(len(A) - 1)"
   ]
